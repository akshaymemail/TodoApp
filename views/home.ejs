<%- include('header') %> 
<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>
<div class="box">
    <% newListItem.forEach(function(item) {%>
    <div class="item">
        <input type="checkbox">
        <p><%= item %> </p>
    </div>
    <% }) %>
    <form class="item" action="/" method="post">
        <input type="text" name="newItem" autofocus placeholder="New Item" autocomplete="off" required>
        <button name="list" value="<%= listTitle %> " type="submit">+</button>
    </form>
</div>

<%- include('footer') %> 